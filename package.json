{
  "name": "@eject/monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "starters/*",
    "generators/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "interface": "pnpm --filter=interface"
  },
  "devDependencies": {
    "prettier": "^2.8.4",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
