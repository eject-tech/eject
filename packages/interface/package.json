{
  "name": "@eject/interface",
  "version": "1.7.1",
  "type": "module",
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "CHANGELOG.md",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc && node build/generate-json-schemas.js",
