{
  "name": "@eject/cli",
  "description": "Command-line interface for developing and building with Eject.",
  "version": "1.3.1",
  "type": "module",
  "exports": "./build/cli.js",
  "types": "./build/cli.d.ts",
  "license": "MIT",
  "bin": {
    "eject": "build/cli.js"
  },
  "files": [
    "build",
    "bin",
    "CHANGELOG.md",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc && node ./build/cli.js build",
    "dev": "tsc --watch",
    "test": "ava"
  },
  "dependencies": {
    "cosmiconfig": "^8.3.6",
    "execa": "^8.0.1",
    "ink": "^4.1.0",
    "ink-spinner": "^5.0.0",
    "meow": "^11.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@eject/config": "workspace:*",
    "@eject/fastify": "workspace:*",
    "@eject/interface": "workspace:*",
    "@fastify/type-provider-typebox": "^3.0.0",
    "@sinclair/typebox": "^0.26.8",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "fastify": "^4.15.0",
    "ink-testing-library": "^3.0.0",
    "ts-node": "^10.9.1",
    "ts-paths-esm-loader": "^1.4.3",
    "typescript": "^5.0.3"
  },
  "ava": {
    "verbose": false,
    "nodeArguments": [
      "--loader=ts-paths-esm-loader"
    ],
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "timeout": "2m"
  }
}
